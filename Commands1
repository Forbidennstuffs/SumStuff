-- Page Functions, Buttons, Texts And Images:

function ScriptRigType(Script)
if Script == "All" then
return "R6, R15"
elseif Script == "R6" then
return "R6"
elseif Script == "R15" then
return "R15"
end
end

function ScriptSupportedGame(PlaceId)
if PlaceId == 0 then
return "Universal"
else
return game:GetService("MarketplaceService"):GetProductInfo(PlaceId).Name
end
end

function JoinGame(PlaceId)
if PlaceId == 0 then
Notification:Create("Whoops!", "You Don't Need To Join A Game To Execute This Script. This Script Is Universal!",5)
elseif game.PlaceId == PlaceId then
Notification:Create("Whoops!", "You're Already In The Game.",5)
else
local GameName = game:GetService("MarketplaceService"):GetProductInfo(PlaceId).Name
Notification:Create("Teleporting...", "Teleporting To "..GameName..". Might Take Seconds.",5)

game:GetService("TeleportService"):Teleport(PlaceId)
end
end

if TabName == "Scripts" then
MainFrame.TabInfo.TabIcon.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton1.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton1.ImageLabel.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton2.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton2.ImageLabel.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton3.Image = "rbxassetid://11592851654"
MainFrame.ChooseScriptFrameParent.ScriptButton3.ImageLabel.Image = "rbxassetid://11592851654"
