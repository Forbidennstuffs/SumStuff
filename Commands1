MainFrame.ChooseScriptFrameParent.ScriptButton1.MouseButton1Down:connect(function()
DisappearOtherFrames()
if getgenv().OnCooldown == false then
getgenv().OnCooldown = true
coroutine.wrap(function()
while wait() do
MainFrame.ExecuteButtons.Execute1.Visible = true
MainFrame.ExecuteButtons.Execute2.Visible = false
MainFrame.ExecuteButtons.Execute3.Visible = false
MainFrame.ExecuteButtons.Execute4.Visible = false
MainFrame.ExecuteButtons.Execute5.Visible = false
MainFrame.ExecuteButtons.Execute6.Visible = false
MainFrame.PlayButtons.Play1.Visible = true
MainFrame.PlayButtons.Play2.Visible = false
MainFrame.PlayButtons.Play3.Visible = false
MainFrame.PlayButtons.Play4.Visible = false
MainFrame.PlayButtons.Play5.Visible = false
MainFrame.PlayButtons.Play6.Visible = false

MainFrame.ScriptInfo.ScriptDetails.Text = '<b><font size="13">'..Script1Name..'</font></b>\n<font size="8"><font color="rgb(200,200,200)">By </font><b>'..Script1Creator..'</b> ('..Script1CreatorPlatform..')</font>\n\n\n<font size="8"><font color="rgb(200,200,200)">"'..Script1Description..'"</font>\n\n\n<font color="rgb(200,200,200)">Game:</font> <b>'..ScriptSupportedGame(Script1GameID)..'</b>\n<font color="rgb(200,200,200)">RigType:</font> <b>'..ScriptRigType(Script1RigType)..'</b></font>'
wait(1)
getgenv().OnCooldown = false
return
end
end)()
else
Notification:Create("Hey!", "Slowdown For 1 Seconds Please!",5)
end
end)

