TweenService:Create(SettingsPopUpFrame.SettingsText, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 0}):Play()
TweenService:Create(SettingsPopUpFrame.Close, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 0.9}):Play()
TweenService:Create(SettingsPopUpFrame.Close, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 0}):Play()
