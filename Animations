local Play = {}

function Play:Create(Animation, Tab, Page, Type)

local ReportPopUpFrame = game:GetService("CoreGui").VexHubFolder.DragScreen.DragImage.ReportPopUp
local SettingsPopUpFrame = game:GetService("CoreGui").VexHubFolder.DragScreen.DragImage.SettingsPopUp
local HomePageFrame = game:GetService("CoreGui").VexHubFolder.DragScreen.DragImage.HomePage
local DragImagePath = game:GetService("CoreGui").VexHubFolder.DragScreen.DragImage
local TweenService = game:GetService("TweenService")

if Animation == "Appear" and Tab == "DragImage" and Page == "0" and Type == "1" then
DragImagePath.BackgroundTransparency = 1
DragImagePath.ImageTransparency = 1
DragImagePath.Visible = true
TweenService:Create(DragImagePath, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 0}):Play()
TweenService:Create(DragImagePath, TweenInfo.new(getgenv().AnimationDelay), {ImageTransparency = 0.5}):Play()

elseif Animation == "Disappear" and Tab == "DragImage" and Page == "0" and Type == "1" then
coroutine.wrap(function()
while wait() do
wait(getgenv().AnimationDelay)
DragImagePath.Visible = false
return
end
end)()
DragImagePath.BackgroundTransparency = 0
DragImagePath.ImageTransparency = 0.5
TweenService:Create(DragImagePath, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 1}):Play()
TweenService:Create(DragImagePath, TweenInfo.new(getgenv().AnimationDelay), {ImageTransparency = 1}):Play()

elseif Animation == "Appear" and Tab == "ReportPopUp" and Page == "0" and Type == "1" then
ReportPopUpFrame.Position = UDim2.new(-0.509999981, 0, 0, 0)
ReportPopUpFrame.BackgroundTransparency = 1
ReportPopUpFrame.ImageTransparency = 1
ReportPopUpFrame.SupportText.TextTransparency = 1
ReportPopUpFrame.TextBoxParent.BackgroundTransparency = 1
ReportPopUpFrame.TextBoxParent.UIStroke.Transparency = 1
ReportPopUpFrame.TextBoxParent.TextBox.TextTransparency = 1
ReportPopUpFrame.SendButton.TextTransparency = 1
ReportPopUpFrame.SendButton.BackgroundTransparency = 1
ReportPopUpFrame.SendButton.UIStroke.Transparency = 1
ReportPopUpFrame.Visible = true
ReportPopUpFrame:TweenPosition(UDim2.new(-0.519999981, 0, 0, 0),"Out","Quad",getgenv().AnimationDelay)
TweenService:Create(ReportPopUpFrame, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 0}):Play()
TweenService:Create(ReportPopUpFrame, TweenInfo.new(getgenv().AnimationDelay), {ImageTransparency = 0.5}):Play()
TweenService:Create(ReportPopUpFrame.SupportText, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 0}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 0.9}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent.UIStroke, TweenInfo.new(getgenv().AnimationDelay), {Transparency = 0.7}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent.TextBox, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 0}):Play()
TweenService:Create(ReportPopUpFrame.SendButton, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 0}):Play()
TweenService:Create(ReportPopUpFrame.SendButton, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 0.9}):Play()
TweenService:Create(ReportPopUpFrame.SendButton.UIStroke, TweenInfo.new(getgenv().AnimationDelay), {Transparency = 0.7}):Play()

elseif Animation == "Disappear" and Tab == "ReportPopUp" and Page == "0" and Type == "1" then
coroutine.wrap(function()
while wait() do
wait(getgenv().AnimationDelay)
ReportPopUpFrame.Visible = false
return
end
end)()

ReportPopUpFrame.Position = UDim2.new(-0.519999981, 0, 0, 0)
ReportPopUpFrame.BackgroundTransparency = 0
ReportPopUpFrame.ImageTransparency = 0.5
ReportPopUpFrame.SupportText.TextTransparency = 0
ReportPopUpFrame.TextBoxParent.BackgroundTransparency = 0.9
ReportPopUpFrame.TextBoxParent.UIStroke.Transparency = 0.7
ReportPopUpFrame.TextBoxParent.TextBox.TextTransparency = 0
ReportPopUpFrame.SendButton.TextTransparency = 0
ReportPopUpFrame.SendButton.BackgroundTransparency = 0.9
ReportPopUpFrame.SendButton.UIStroke.Transparency = 0.7
ReportPopUpFrame:TweenPosition(UDim2.new(-0.509999981, 0, 0, 0),"Out","Quad",getgenv().AnimationDelay)
TweenService:Create(ReportPopUpFrame, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame, TweenInfo.new(getgenv().AnimationDelay), {ImageTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.SupportText, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent.UIStroke, TweenInfo.new(getgenv().AnimationDelay), {Transparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.TextBoxParent.TextBox, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.SendButton, TweenInfo.new(getgenv().AnimationDelay), {TextTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.SendButton, TweenInfo.new(getgenv().AnimationDelay), {BackgroundTransparency = 1}):Play()
TweenService:Create(ReportPopUpFrame.SendButton.UIStroke, TweenInfo.new(getgenv().AnimationDelay), {Transparency = 1}):Play()

elseif Animation == "Appear" and Tab == "SettingsPopUp" and Page == "0" and Type == "1" then
SettingsPopUpFrame.Position = UDim2.new(1.01499998, 0, 0, 0)
SettingsPopUpFrame.BackgroundTransparency = 1
