SettingsPopUpFrame.Settings2.TopScreenButton.UIStroke.Transparency = 1
SettingsPopUpFrame.Settings2.FloatingButton.BackgroundTransparency = 1
SettingsPopUpFrame.Settings2.FloatingButton.TextTransparency = 1
SettingsPopUpFrame.Settings2.FloatingButton.UIStroke.Transparency = 1
SettingsPopUpFrame.Settings2.AnimationsText.TextTransparency = 1
SettingsPopUpFrame.Settings2.FastButton.BackgroundTransparency = 1
SettingsPopUpFrame.Settings2.FastButton.TextTransparency = 1
SettingsPopUpFrame.Settings2.FastButton.UIStroke.Transparency = 1
SettingsPopUpFrame.Settings2.SlowButton.BackgroundTransparency = 1
SettingsPopUpFrame.Settings2.SlowButton.TextTransparency = 1
SettingsPopUpFrame.Settings2.SlowButton.UIStroke.Transparency = 1
SettingsPopUpFrame.Settings2.Next.BackgroundTransparency = 1
SettingsPopUpFrame.Settings2.Next.TextTransparency = 1
